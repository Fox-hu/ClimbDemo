<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.xingfeng.jetpackdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".animation.AnimationDrawableActivity">
        </activity>
        <activity android:name=".animation.CrossFadeActivity">
        </activity>
        <activity android:name=".animation.CardFlipActivity">
        </activity>
        <activity android:name=".animation.CircleRevealActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>