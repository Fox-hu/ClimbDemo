<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.xingfeng.jetpackdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JetpackMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".architecture.databinding.FirstExampleActivity">
        </activity>
        <activity android:name=".architecture.databinding.DataBindingMainActivity">
        </activity>
        <activity android:name=".animation.MainActivity">
        </activity>
        <activity android:name=".animation.AnimationDrawableActivity">
        </activity>
        <activity android:name=".animation.CrossFadeActivity">
        </activity>
        <activity android:name=".animation.CardFlipActivity">
        </activity>
        <activity android:name=".animation.CircleRevealActivity">
        </activity>
        <activity android:name=".animation.MoveViewActivity">
        </activity>
        <activity android:name=".animation.EnlargeImageActivity">
        </activity>
        <activity android:name=".animation.layoutanimate.LayoutAnimateActivity">
        </activity>
        <activity android:name=".animation.layoutanimate.TransitionLayoutActivity">
        </activity>
        <activity android:name=".animation.layoutanimate.CustomTransitionActivity">
        </activity>
        <activity android:name=".animation.activitytransition.ActivityTransitionActivity">
        </activity>
        <activity android:name=".animation.activitytransition.FadeActivity">
        </activity>
        <activity android:name=".animation.activitytransition.ExplodeActivity">
        </activity>
        <activity android:name=".animation.activitytransition.SlideActivity">
        </activity>
        <activity android:name=".animation.activitytransition.ShareViewActivity">
        </activity>
        <activity android:name=".animation.physicsanimation.FlingAnimationActivity">
        </activity>
        <activity android:name=".animation.physicsanimation.PhysicsAnimationActivity">
        </activity>
        <activity android:name=".animation.physicsanimation.SpringAnimationActivity">
        </activity>
        <activity android:name=".animation.physicsanimation.ChainedSpringActivity">
        </activity>
        <activity
            android:name=".animation.physicsanimation.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".animation.propertyanimator.PropertyAnimatorActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.ViewAnimationActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.ValueAnimatorActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.ObjectAnimatorActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.AnimatorSetActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.InterpolatorActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.StateListAnimatorActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.BreathViewActivity">
        </activity>
        <activity android:name=".animation.activitytransition.ScaleUpActivity">
        </activity>
        <activity android:name=".animation.activitytransition.ThumbnailScaleUpActivity">
        </activity>
        <activity android:name=".animation.propertyanimator.KeyFrameActivity">
        </activity>
    </application>

</manifest>